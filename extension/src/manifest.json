{
    "name": "<webpack managed>",
    "version": "<webpack managed>",
    "description": "Indicates whether and when the page was visited (and more!)",
    "web_accessible_resources": [
        "sidebar.js",
        "sidebar.css",
        "shallalist/*"
    ],
    "background": {
        "scripts": ["background.js", "sidebar.js"],
        "persistent": false
    },
    "options_ui": {
        "page": "options_page.html",
        "chrome_style": true
    },

    "permissions": [
        "file:///*",
        "https://*/*",
        "http://*/*",

        "storage",

        "tabs",
        "history",

        "activeTab",

        "webNavigation",

        "contextMenus",

        "notifications"
    ],
    "icons": {
        "48": "images/ic_not_visited_48.png"
    },
    "commands": {
        "_execute_browser_action": {
            "description": "Activate extension"
        },
        "show_dots": {
            "description": "Activate dots for visited urls"
        },
        "search": {
            "description": "Open search page"
        }
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
