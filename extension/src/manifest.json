{
    "name": "<webpack managed>",
    "version": "<webpack managed>",
    "description": "Indicates whether and when the page was visited (and more!)",
    "web_accessible_resources": ["sidebar.js", "sidebar.css"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "options_ui": {
        "page": "options_page.html",
        "chrome_style": true
    },

    "permissions": [
        "file://*",
        "https://*/*",
        "http://*/*",

        "storage",

        "tabs",
        "history",

        "activeTab",

        "notifications",

        "webNavigation"
    ],
    "icons": {
        "48": "images/ic_not_visited_48.png"
    },
    "browser_action": {
        "default_icon": "images/ic_not_visited_48.png",
        "default_title": "Was not visited"
    },
    "page_action": {
        "default_icon": "images/ic_not_visited_48.png",
        "default_title": "Was not visited"
    },
    "commands": {
        "_execute_browser_action": {
            "description": "Activate extension",
            "suggested_key": {
                "default": "Ctrl+Alt+W",
                "mac"    : "Command+Alt+W"
            }
        },
        "show_dots": {
            "description": "Activate dots for visited urls",
            "suggested_key": {
                "default": "Ctrl+Alt+V",
                "mac"    : "Command+Alt+V"
            }
        }
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
